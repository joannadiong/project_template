journal = {J. Neurophysiol.},
keywords = {Action Potentials,Animals,Biological Clocks,Cats,Decerebrate State,Decerebrate State: physiopathology,Gait,Locomotion,Motor Neurons,Spinal Cord,Spinal Cord: physiopathology},
month = mar,
number = {3},
pages = {1045--50},
pmid = {21177992},
