file = {:home/martin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/B\"{a}hring et al. - 2001 - Kinetic analysis of open- and closed-state inactivation transitions in human Kv4.2 A-type potassium channels.pdf:pdf},
issn = {0022-3751},
journal = {J. Physiol.},
keywords = {Cell Line,Computer Simulation,Electric Conductivity,Gene Deletion,Humans,Ion Channel Gating,Kinetics,Markov Chains,Models, Biological,Mutation,Mutation: physiology,Potassium Channels,Potassium Channels, Voltage-Gated,Potassium Channels: genetics,Potassium Channels: physiology,Shal Potassium Channels},
