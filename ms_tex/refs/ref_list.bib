keywords = {Adolescent,Adult,Brain,Brain: physiology,Electromyography,Female,Humans,Isometric Contraction,Male,Middle Aged,Muscle Contraction,Muscles,Muscles: physiology,Physical Endurance,Time Factors},
month = aug,
number = {2},
pages = {421--9},
pmid = {3745035},
title = {{Fatigue of intermittent submaximal voluntary contractions: central and peripheral factors.}},
